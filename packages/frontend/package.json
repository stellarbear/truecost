		"@material-ui/icons": "^4.2.1",
		"@material-ui/lab": "^4.0.0-alpha.19",
		"@material-ui/pickers": "^3.2.8",
		"@stripe/stripe-js": "^1.0.0-beta.6",
		"apollo-boost": "^0.4.4",
		"apollo-link-error": "^1.1.12",
		"apollo-upload-client": "^13.0.0",
		"axios": "^0.19.0",
		"dayjs": "^1.8.18",
		"express": "^4.17.1",
		"global": "^4.4.0",
		"graphql": "^15.1.0",
		"history": "^4.10.1",
		"isomorphic-fetch": "^2.2.1",
		"markdown-to-jsx": "^6.11.0",
		"mdi-material-ui": "^6.11.0",
		"pigeon-maps": "^0.15.0",
		"pigeon-overlay": "^0.2.3",
		"razzle": "^3.1.4",
		"razzle-plugin-typescript": "^3.1.4",
		"react": "^16.12.0",
		"react-apollo": "^3.1.3",
		"react-dom": "^16.12.0",
		"react-helmet": "^6.1.0",
		"react-hook-form": "^6.0.1",
		"react-i18next": "^11.2.5",
		"react-lazyload": "^2.6.5",
		"react-router-dom": "^5.1.2",
		"react-smooth": "^1.0.5",
		"react-sortablejs": "^2.0.7",
		"react-spring": "^8.0.27",
		"react-text-fun": "^1.0.0",
		"react-text-loop": "^2.3.0",
		"react-twitter-embed": "^3.0.3",
		"react-use-gesture": "^7.0.4",
		"react-yandex-maps": "^4.2.0",
		"uuid": "^8.2.0",
		"window": "^4.2.6"
	},
	"devDependencies": {
		"@types/apollo-upload-client": "^8.1.3",
		"@types/express": "^4.17.2",
		"@types/graphql": "^14.5.0",
		"@types/history": "^4.7.3",
		"@types/isomorphic-fetch": "^0.0.35",
		"@types/markdown-to-jsx": "^6.9.1",
		"@types/node": "^14.0.13",
		"@types/react": "^16.9.39",
		"@types/react-dom": "^16.9.4",
